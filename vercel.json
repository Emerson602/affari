{
  "version": 2,
  "builds": [
    {
      "src": "./backend/sendEmail/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./backend/sendEmail/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./frontend/src/App.vue"
    }
  ]
}

